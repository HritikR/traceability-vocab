        ],
        "type": "ChemicalProperty",
        "name": "Magnesium",
        "formula": "Mg",
        "inchi": "InChI=1S/Mg",
        "inchikey": "FYYHWMGAXLPEAU-UHFFFAOYSA-N"
      },
      {
        "@context": [
          "https://w3id.org/traceability/v1"
        ],
        "type": "ChemicalProperty",
        "name": "Germanium",
        "formula": "Ge",
        "inchi": "InChI=1S/Ge",
        "inchikey": "GNPVGFCGXDBREM-UHFFFAOYSA-N"
      }
    ],
